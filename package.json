{
  "homepage": "https://www.composita.dev/",
  "name": "composita.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predeploy": "yarn build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "build": "yarn lint && react-scripts build && copyfiles CNAME .nojekyll LICENSE.txt build",
    "eject": "react-scripts eject",
    "svgr": "svgr --ignore-existing --out-dir './src/assets/svg/' --typescript --icon --ext 'tsx' --filename-case 'kebab'",
    "update:dev": "yarn add @composita/system@dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@composita/system": "^0.1.0-dev.17",
    "bootstrap": "^4.5.0",
    "codemirror": "^5.56.0",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "webpack": "4.42.0",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@composita/ts-utility-types": "^0.1.0-dev.7",
    "@svgr/cli": "^5.4.0",
    "@types/bootstrap": "^4.5.0",
    "@types/codemirror": "0.0.97",
    "@types/jest": "^26.0.5",
    "@types/jquery": "^3.5.0",
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "copyfiles": "^2.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "popper.js": "^1.16.1",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1",
    "typescript": "~3.9.7"
  }
}
