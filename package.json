{
  "homepage": "https://www.composita.dev/",
  "name": "composita.github.io",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "predeploy": "yarn build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "build": "yarn lint && react-scripts build && copyfiles CNAME .nojekyll LICENSE.txt build",
    "eject": "react-scripts eject",
    "svgr": "svgr --ignore-existing --out-dir './src/assets/svg/' --typescript --icon --ext 'tsx' --filename-case 'kebab'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@composita/system": "^0.2.1",
    "bootstrap": "^4.5.2",
    "codemirror": "^5.58.1",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "webpack": "4.42.0",
    "worker-loader": "^3.0.3"
  },
  "devDependencies": {
    "@composita/ts-utility-types": "^0.2.0",
    "@svgr/cli": "^5.4.0",
    "@types/bootstrap": "^4.5.0",
    "@types/codemirror": "0.0.98",
    "@types/jquery": "^3.5.1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "copyfiles": "^2.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "popper.js": "^1.16.1",
    "prettier": "^2.1.2",
    "react-scripts": "^3.4.3",
    "typescript": "~4.0.3"
  }
